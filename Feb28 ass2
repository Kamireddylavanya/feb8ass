assignment-2
def count(arr, n):
   visited = [False for i in range(n)]
   for i in range(n):
     if (visited[i] == True):
        continue
     count = 1
     for j in range(i + 1, n, 1):
        if (arr[i] == arr[j]):
          visited[j] = True
          count += 1
     if count == 1 :
        print(arr[i])
arr = [1,2,1,3,2,4,2,5,4,6,5,7,6,4]
n = len(arr)
count(arr, n)
